[push]
	default = simple
[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	pl = pull
	ps = push
	di = diff --color-words
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	type = cat-file -t
	dump = cat-file -p
[advice]
	pushNonFastForward = false
	statusHints = false
	commitBeforeMerge = false
	resolveConflict = false
	implicitIdentity = false
	detachedHead = false
